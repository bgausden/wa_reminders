    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "env": {
                "NODE_ENV": "development"
            },
            "program": "${workspaceFolder}/dist/index.js",
            "outFiles": [
                "${workspaceFolder}/dist/**/*",
                "!**/node_modules/**"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
        }
    ]
}
